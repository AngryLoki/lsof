#!/bin/sh
#
# Compare lsof Darwin libproc sources

A=$HOME/src/lsof-Apple/dialects/darwin
S=$HOME/src/lsof4/dialects/darwin

cd $S
echo "==== $S ===="
for i in get-hdr-loc.sh
do
  echo "*** $i ***"
  if test -r $A/$i
  then
    diff -c $A/$i $i
  else
    echo "!!!! $i doesn't exist in $A !!!!"
  fi
done
