@#!/bin/ksh
#
# OSFDistrib -- make Digital UNIX (DEC OSF/1) distribution of lsof 4.x
#
# Usage: DUDistrib [<suffix>]
#
#	 <suffix>	optional suffix for identification

if test $# -gt 1
then
  echo "Usage: [<suffix>]"
  exit 1
fi
$HOME/src/lsof4/support/GenericDistrib du $1
@
