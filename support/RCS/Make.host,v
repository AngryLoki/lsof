text
@`#	$Id: Make.host,v 1.2 1997/10/17 13:45:48 abe Exp abe $
#
#	Configuration makefile for lsof 4
#

CFTY=  'ifelse(
HOSTTYPE,`IBMR2',`aix',
HOSTTYPE,`SUN5',`solaris',
HOSTTYPE,`SUN5CUE',`solaris')`

RPT=	make -f Makefile $@@

all:	source
	${RPT}

clean:	source
	./Configure -clean

deinstall: source
	${RPT}

depend:	source
	${RPT}

dirs:	source
	${RPT}

distrib: source
	${RPT}

print:	source
	${RPT}

source:	FRC
	rm -f Makefile Makefile.bak
	sh -c "LSOF_LOCALSUFFIX=LOCAL && export LSOF_LOCALSUFFIX && \
	       LSOF_DISTRIBKVM='KVM` && export LSOF_DISTRIBKVM  && \
	       ./Configure -n ${CFTY}"
	mv makefile Makefile.bak

FRC:
'dnl
@


